{
    "app_listening_port": 8080,
    "default_folder_name": "@default",
    "mock_file_not_found_http_code": 404,
    "enable_extra_http_responses_headers": {
        "mock_file_not_found": true,
        "mock_file_is_found_and_ok": true,
        "mock_file_is_found_and_empty": true,
        "mock_file_is_found_and_unreadable": true
    },
    "listen_to_these_api_base_url": {
        "api1": "data/api",
        "api2": "/data/clientName/api2"
    },
    "mock_files_authorized_extensions": ["json","phpmock"],
    "enable_php_for_these_extensions": ["phpmock"]
}
